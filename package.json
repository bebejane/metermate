    "@graphql-typed-document-node/core": "^3.2.0",
    "@hookform/resolvers": "^5.0.1",
    "@next/env": "^15.3.2",
    "@next/third-parties": "^15.3.4",
    "@u-wave/react-vimeo": "^0.9.12",
    "classnames": "^2.5.1",
    "date-fns": "^2.30.0",
    "datocms-structured-text-to-plain-text": "^5.0.0",
    "dotenv": "^16.5.0",
    "google": "link:@next/third-parties/google",
