    "next-intl": "^4.3.5",
    "nuqs": "^2.4.3",
    "react-datocms": "8.0.0-1",
    "react-hook-form": "^7.56.4",
    "react-use": "^17.6.0",
    "react-youtube": "^10.1.0",
    "rooks": "^7.14.1",
    "sass": "1.80.4",
    "swiper": "^11.2.8",
    "zod": "^3.25.32",
    "zustand": "^5.0.5"
