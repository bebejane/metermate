    "@types/node": "^22.15.23",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "chokidar-cli": "^3.0.0",
    "eslint": "^9.27.0",
